{
    "extends": [
        "prettier"
    ],
    "plugins": [
        "prettier",
        "prettier-plugin-organize-imports"
    ],
    "rules": {
        "prettier/prettier": [
            "error"
        ]
    },
    "env": {
        "node": true,
        "es2021": true
    },
    "parserOptions": {
        "ecmaVersion": 15
    }
}